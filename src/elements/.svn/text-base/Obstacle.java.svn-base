package elements;

import application.Emergence;
import elements.abstracts.Element;

public class Obstacle extends Element {

	public Obstacle(int x, int y) {
		super(x, y, Emergence.OBSTACLE_SIZE);
		
	}
	
	public void draw(){
		app.fill(150);
		app.rect(x, y, Emergence.OBSTACLE_SIZE, Emergence.OBSTACLE_SIZE);	
	}

	

}
